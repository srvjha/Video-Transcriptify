{
    "version": 2,
    "builds": [{ "src": "src/main.js", "use": "@vercel/node" }],
    "routes": [{ "src": "/(.*)", "dest": "src/main.js" }],
    "build": {
      "env": {
        "NODE_ENV": "production"
      },
      "commands": [
        "npm install", 
        "npm run build"
      ]
    }
  }
  